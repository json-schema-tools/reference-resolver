{
  "name": "@json-schema-tools/reference-resolver",
  "mode": "file",
  "inputFiles": [
    "./src/index.ts"
  ],
  "out": "docs",
  "readme": "./README.md",
  "exclude": [
    "node_modules",
    "docs",
    "build",
    "jest.config.js",
    "src/*.test.ts"
  ],
  "toc": [
    "referenceResolver",
    "InvalidFileSystemPathError",
    "InvalidJsonPointerRefError",
    "NonJsonRefError"
  ]
}
